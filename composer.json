{
	"name": "tatter/sefer",
	"type": "project",
	"description": "The Sefer Project",
	"keywords": [
		"codeigniter",
		"codeigniter4"
	],
	"homepage": "https://sefer.red",
	"license": "GNU General Public License",
	"authors": [
		{
			"name": "Tatter Software",
			"email": "support@tattersoftware.com",
			"homepage": "https://tattersoftware.com",
			"role": "Owner"
		}
	],
	"require": {
		"php": ">=7.2",
		"codeigniter4/codeigniter4": "dev-develop"
	},
	"require-dev": {
		"tatter/tools": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Organization\\\\Name\\\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Support\\": "tests/_support"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/codeigniter4/codeigniter4"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-update-cmd": [],
		"analyze": "phpstan analyze",
		"style": "phpcs --standard=./vendor/codeigniter4/codeigniter4-standard/CodeIgniter4 tests/ app/",
		"test": "phpunit"
	}
}
